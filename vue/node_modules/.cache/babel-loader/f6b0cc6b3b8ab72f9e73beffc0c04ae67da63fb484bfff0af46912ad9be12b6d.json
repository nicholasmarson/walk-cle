{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport authService from \"../services/AuthService\";\nexport default {\n  name: \"login\",\n  components: {},\n  data: function data() {\n    return {\n      user: {\n        username: \"\",\n        password: \"\"\n      },\n      invalidCredentials: false\n    };\n  },\n  methods: {\n    login: function login() {\n      var _this = this;\n      authService.login(this.user).then(function (response) {\n        if (response.status == 200) {\n          _this.$store.commit(\"SET_AUTH_TOKEN\", response.data.token);\n          _this.$store.commit(\"SET_USER\", response.data.user);\n          _this.$router.push(\"/\");\n        }\n      })[\"catch\"](function (error) {\n        var response = error.response;\n        if (response.status === 401) {\n          _this.invalidCredentials = true;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA2BA;AAEA;EACAA;EACAC;EACAC;IACA;MACAC;QACAC;QACAC;MACA;MACAC;IACA;EACA;EACAC;IACAC;MAAA;MACAC,YACAD,iBACAE;QACA;UACA;UACA;UACA;QACA;MACA,WACA;QACA;QAEA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","components","data","user","username","password","invalidCredentials","methods","login","authService","then"],"sourceRoot":"src/views","sources":["Login.vue"],"sourcesContent":["<template>\r\n  <div id=\"login\">\r\n     <!-- put logo here -->\r\n    <h1>Please Sign In</h1>\r\n    <form @submit.prevent=\"login\">\r\n      <div role=\"alert\" v-if=\"invalidCredentials\">\r\n        Invalid username and password!\r\n      </div>\r\n      <div role=\"alert\" v-if=\"this.$route.query.registration\">\r\n        Thank you for registering, please sign in.\r\n      </div>\r\n      <div class=\"form-input-group\">\r\n        <label for=\"username\">Username</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"user.username\" required autofocus />\r\n      </div>\r\n      <div class=\"form-input-group\">\r\n        <label for=\"password\">Password</label>\r\n        <input type=\"password\" id=\"password\" v-model=\"user.password\" required />\r\n      </div>\r\n      <button type=\"submit\" class=\"\">Sign in</button>\r\n      <p>\r\n      <router-link :to=\"{ name: 'register' }\">Need an account? Sign up.</router-link></p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport authService from \"../services/AuthService\";\r\n\r\nexport default {\r\n  name: \"login\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: \"\",\r\n        password: \"\"\r\n      },\r\n      invalidCredentials: false\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      authService\r\n        .login(this.user)\r\n        .then(response => {\r\n          if (response.status == 200) {\r\n            this.$store.commit(\"SET_AUTH_TOKEN\", response.data.token);\r\n            this.$store.commit(\"SET_USER\", response.data.user);\r\n            this.$router.push(\"/\");\r\n          }\r\n        })\r\n        .catch(error => {\r\n          const response = error.response;\r\n\r\n          if (response.status === 401) {\r\n            this.invalidCredentials = true;\r\n          }\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-input-group {\r\n  margin-bottom: 1rem;\r\n}\r\nlabel {\r\n  margin-right: 0.5rem;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}